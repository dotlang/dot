main := () -> int
{
    assert( func1(8) = 9 )
    assert( func2(3.99) = 3 )
    assert( func3('a') = 97 )
    assert( adder(1,2) = 3 )
    assert( subb(10,3) = 7)
    assert( subb(30, 40) = -10)

    :: 0
}

func1 := (i: int) -> i+1

func2 := (x: float ) -> int
{
    :: int(x)
}

func3 := (c: char ) -> int
{
    :: int(c)
}

adder := (f:int, g:int) -> int
{
    :: f + g
}

subb := (o:int, p:int) -> o-p
